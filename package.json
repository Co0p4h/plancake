		"vite": "^6.3.5"
	},
	"dependencies": {
		"@inlang/paraglide-js": "^2.2.0",
		"@lucide/svelte": "^0.511.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sveltejs/adapter-node": "^5.2.13",
		"arctic": "^3.7.0",
		"dayjs": "^1.11.13",
		"drizzle-orm": "^0.43.1",
